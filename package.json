{
  "name": "redis_chatroom",
  "version": "1.0.0",
  "description": "Redis Chatroom is a web based chatroom hosted on a web server writeen in node.js. The user and message data is stored in a redis database. A rudimentary html front end is also provided.",
  "main": "myWebServer.js",
  "scripts": {
    "test": "mocha",
    "start": "(npm i) && (concurrently --names 'REDIS,SERVER' --prefix-colors 'yellow,blue' --kill-others 'redis-server redis.conf' 'node myWebServer.js')"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bnwatch64@github.com/bnwatch64/Redis_Chatroom.git"
  },
  "author": "Benjamin Ader",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bnwatch64/Redis_Chatroom/issues"
  },
  "homepage": "https://github.com/bnwatch64/Redis_Chatroom#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chai": "^4.3.4",
    "concurrently": "^6.1.0",
    "express": "^4.17.1",
    "mocha": "^8.4.0",
    "node-env-file": "^0.1.8",
    "pug": "^3.0.2",
    "redis": "^3.1.2",
    "socket.io": "^4.1.1"
  }
}
